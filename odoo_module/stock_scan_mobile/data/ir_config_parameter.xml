<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Mobile API Configuration Parameters -->
        <record id="config_mobile_api_enabled" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.api_enabled</field>
            <field name="value">True</field>
        </record>
        
        <record id="config_mobile_api_version" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.api_version</field>
            <field name="value">1.0</field>
        </record>
        
        <record id="config_mobile_debug_mode" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.debug_mode</field>
            <field name="value">False</field>
        </record>
        
        <!-- Authentication Configuration -->
        <record id="config_mobile_token_expiry_hours" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.token_expiry_hours</field>
            <field name="value">24</field>
        </record>
        
        <record id="config_mobile_max_login_attempts" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.max_login_attempts</field>
            <field name="value">5</field>
        </record>
        
        <record id="config_mobile_lockout_duration_minutes" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.lockout_duration_minutes</field>
            <field name="value">15</field>
        </record>
        
        <!-- API Rate Limiting -->
        <record id="config_mobile_api_rate_limit_per_minute" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.api_rate_limit_per_minute</field>
            <field name="value">100</field>
        </record>
        
        <record id="config_mobile_api_burst_limit" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.api_burst_limit</field>
            <field name="value">20</field>
        </record>
        
        <!-- Batch Processing Configuration -->
        <record id="config_mobile_max_batch_size" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.max_batch_size</field>
            <field name="value">100</field>
        </record>
        
        <record id="config_mobile_batch_timeout_seconds" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.batch_timeout_seconds</field>
            <field name="value">30</field>
        </record>
        
        <!-- CORS Configuration -->
        <record id="config_mobile_cors_enabled" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.cors_enabled</field>
            <field name="value">True</field>
        </record>
        
        <record id="config_mobile_cors_origins" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.cors_origins</field>
            <field name="value">*</field>
        </record>
        
        <record id="config_mobile_cors_methods" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.cors_methods</field>
            <field name="value">GET,POST,PUT,DELETE,OPTIONS</field>
        </record>
        
        <record id="config_mobile_cors_headers" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.cors_headers</field>
            <field name="value">Content-Type,Authorization,X-Requested-With</field>
        </record>
        
        <!-- Data Synchronization Configuration -->
        <record id="config_mobile_sync_enabled" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.sync_enabled</field>
            <field name="value">True</field>
        </record>
        
        <record id="config_mobile_sync_interval_minutes" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.sync_interval_minutes</field>
            <field name="value">5</field>
        </record>
        
        <record id="config_mobile_offline_data_retention_days" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.offline_data_retention_days</field>
            <field name="value">30</field>
        </record>
        
        <!-- Barcode Scanning Configuration -->
        <record id="config_mobile_barcode_validation_enabled" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.barcode_validation_enabled</field>
            <field name="value">True</field>
        </record>
        
        <record id="config_mobile_barcode_min_length" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.barcode_min_length</field>
            <field name="value">3</field>
        </record>
        
        <record id="config_mobile_barcode_max_length" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.barcode_max_length</field>
            <field name="value">50</field>
        </record>
        
        <!-- Serial Number Configuration -->
        <record id="config_mobile_serial_auto_create" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.serial_auto_create</field>
            <field name="value">True</field>
        </record>
        
        <record id="config_mobile_serial_duplicate_check" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.serial_duplicate_check</field>
            <field name="value">True</field>
        </record>
        
        <record id="config_mobile_serial_format_validation" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.serial_format_validation</field>
            <field name="value">True</field>
        </record>
        
        <!-- Logging Configuration -->
        <record id="config_mobile_log_api_calls" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.log_api_calls</field>
            <field name="value">True</field>
        </record>
        
        <record id="config_mobile_log_level" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.log_level</field>
            <field name="value">INFO</field>
        </record>
        
        <record id="config_mobile_log_retention_days" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.log_retention_days</field>
            <field name="value">90</field>
        </record>
        
        <!-- Performance Configuration -->
        <record id="config_mobile_cache_enabled" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.cache_enabled</field>
            <field name="value">True</field>
        </record>
        
        <record id="config_mobile_cache_timeout_seconds" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.cache_timeout_seconds</field>
            <field name="value">300</field>
        </record>
        
        <record id="config_mobile_max_concurrent_requests" model="ir.config_parameter">
            <field name="key">stock_scan_mobile.max_concurrent_requests</field>
            <field name="value">50</field>
        </record>
        
    </data>
</odoo>
